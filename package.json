{
    "name": "root",
    "private": true,
    "scripts": {
        "build": "turbo build",
        "start": "turbo start",
        "dev": "turbo dev",
        "lint": "turbo lint",
        "tunnel": "lt --port 3000 --subdomain medusastore",
        "generate-env": "cp ./apps/medusa/.env.template ./apps/medusa/.env && cp ./apps/storefront/.env.template ./apps/storefront/.env",
        "medusa:init": "turbo run medusa:init --filter=medusa",
        "format": "turbo run format",
        "clean": "find . -name \"node_modules\" -type d -prune -exec rm -rf '{}'",
        "typecheck": "turbo run typecheck"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.3",
        "prettier": "^3.2.5",
        "turbo": "^2.1.2",
        "typescript": "^5.6.2"
    },
    "engines": {
        "node": ">=20"
    },
    "resolutions": {
        "@lambdacurry/medusa-product-reviews": "1.2.0",
        "@medusajs/icons": "2.8.4",
        "@medusajs/ui": "4.0.14",
        "react": "19.1.0",
        "@types/react": "18.3.2"
    },
    "packageManager": "yarn@4.5.0",
    "workspaces": [
        "apps/*",
        "packages/*"
    ]
}
